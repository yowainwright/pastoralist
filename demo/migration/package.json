{
  "name": "pastoralist-migration-demo",
  "version": "1.0.0",
  "description": "Demo showing migration from manual overrides to pastoralist",
  "type": "module",
  "scripts": {
    "start": "node --loader tsx index.ts",
    "before": "node --loader tsx show-before.ts",
    "migrate": "node --loader tsx migrate.ts",
    "after": "node --loader tsx show-after.ts"
  },
  "dependencies": {
    "lodash": "4.17.15",
    "express": "4.16.0",
    "react": "17.0.0",
    "axios": "0.21.0",
    "moment": "2.29.0"
  },
  "devDependencies": {
    "pastoralist": "^1.6.0",
    "@types/node": "^24.0.0",
    "tsx": "^4.0.0"
  },
  "overrides": {
    "lodash": "4.17.21",
    "minimist": "1.2.6",
    "glob-parent": "6.0.2",
    "trim-newlines": "3.0.1",
    "node-fetch": "2.6.7"
  },
  "comment": "These overrides were added manually over time without documentation"
}