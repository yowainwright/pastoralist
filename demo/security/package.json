{
  "name": "pastoralist-security-demo",
  "version": "1.0.0",
  "description": "Demo of pastoralist security vulnerability detection and auto-fix",
  "type": "module",
  "scripts": {
    "start": "bun run index.ts",
    "check": "bun run check-security.ts",
    "fix": "bun run auto-fix.ts",
    "interactive": "bun run interactive.ts"
  },
  "dependencies": {
    "lodash": "4.17.20",
    "minimist": "1.2.5",
    "axios": "0.21.0",
    "express": "4.20.0",
    "moment": "2.29.1"
  },
  "devDependencies": {
    "pastoralist": "^1.6.0",
    "@types/bun": "^1.1.0"
  },
  "pastoralist": {
    "security": {
      "enabled": true,
      "provider": "osv",
      "autoFix": false,
      "interactive": false
    }
  }
}