{
  "name": "pastoralist-security-demo",
  "version": "1.0.0",
  "description": "Demo of pastoralist security vulnerability detection and auto-fix",
  "type": "module",
  "scripts": {
    "start": "node --loader tsx index.ts",
    "check": "node --loader tsx check-security.ts",
    "fix": "node --loader tsx auto-fix.ts",
    "interactive": "node --loader tsx interactive.ts"
  },
  "dependencies": {
    "lodash": "4.17.20",
    "minimist": "1.2.5",
    "axios": "0.21.0",
    "express": "4.17.1",
    "moment": "2.29.1"
  },
  "devDependencies": {
    "pastoralist": "^1.6.0",
    "@types/node": "^24.0.0",
    "tsx": "^4.0.0"
  },
  "pastoralist": {
    "security": {
      "enabled": true,
      "provider": "osv",
      "autoFix": false,
      "interactive": false
    }
  }
}