{
  "name": "pastoralist",
  "version": "1.1.12-beta.5",
  "description": "A tool to watch over node module resolutions and overrides ğŸ‘ ğŸ‘©ğŸ½â€ğŸŒ¾",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "pastoralist": "./dist/index.js"
  },
  "scripts": {
    "build": "turbo run build-dist",
    "build-dist": "pnpm run clean-dist && tsup src/index.ts --format esm,cjs --dts",
    "clean-dist": "rimraf dist",
    "commit": "git-cz",
    "commit-msg": "commitlint --edit $1",
    "dev": "pnpm --filter @pastoralist/app run app-dev",
    "lint": "turbo run lint-src",
    "lint-fix": "eslint src --fix",
    "lint-src": "eslint src",
    "pre-commit": "lint-staged --quiet && pnpm run test",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pnpm run test && pnpm run build",
    "preview": "pnpm --filter @pastoralist/app app-preview",
    "release": "release-it",
    "test": "echo 'no test right now'",
    "test-src": "jest --coverage --maxWorkers=75%",
    "test-resolutions": "tsx src/index.ts --debug",
    "typecheck": "turbo run typecheck-src",
    "typecheck-src": "tsc --noEmit",
    "update": "codependence --update && pnpm run update-app-deps",
    "update-app-deps": "pnpm --filter @pastoralist/app update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yowainwright/pastoralist.git"
  },
  "keywords": [
    "resolutions",
    "overrides",
    "dependencies",
    "security",
    "dependency-management",
    "yarn",
    "pnpm"
  ],
  "author": "Jeff Wainwright <https:jeffry.in> (yowainwright@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yowainwright/pastoralist/issues"
  },
  "homepage": "https://jeffry.in/pastoralist/",
  "dependencies": {
    "commander": "12.1.0",
    "compare-versions": "6.1.1",
    "fast-glob": "3.3.2",
    "gradient-string": "^2.0.2",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@commitlint/format": "^19.3.0",
    "@eslint/js": "^9.1.1",
    "@swc/core": "^1.4.17",
    "@swc/jest": "^0.2.36",
    "@types/gradient-string": "^1.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.11",
    "codependence": "^0.2.6",
    "eslint": "9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "9.1.1",
    "is-ci": "^3.0.1",
    "jest": "^29.7.0",
    "lint-staged": "15.2.7",
    "path-exists-cli": "^2.0.0",
    "prettier": "3.3.3",
    "release-it": "17.6.0",
    "rimraf": "6.0.1",
    "stdouttojson": "1.0.0",
    "ts-node": "10.9.2",
    "tslib": "2.6.3",
    "tsup": "8.1.2",
    "tsx": "^4.7.3",
    "turbo": "2.0.7",
    "typescript": "5.5.3",
    "typescript-eslint": "^7.7.1",
    "vite": "5.3.4"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|ts)$": "@swc/jest"
    },
    "testRegex": "(src/__tests__/.*|(\\.|/)(test))\\.(ts?)$",
    "testEnvironment": "jest-environment-node",
    "clearMocks": true,
    "moduleFileExtensions": [
      "js",
      "ts",
      "json"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.ts"
    ]
  },
  "eslintConfig": {
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release"
    }
  },
  "packageManager": "pnpm@9.5.0",
  "pastoralist": {
    "appendix": {
      "json5@2.2.2": {
        "dependents": {
          "stdouttojson": "json5@^2.2.3"
        }
      },
      "react@^18.2.0": {
        "dependents": {
          "@tailwindcss/typography": "react@^17.0.2"
        }
      },
      "react-dom@^18.2.0": {
        "dependents": {
          "@tailwindcss/typography": "react-dom@^17.0.2"
        }
      }
    }
  }
}
