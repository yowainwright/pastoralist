{
  "name": "pastoralist-sandbox-quiet",
  "private": true,
  "type": "module",
  "bin": {
    "pastoralist": "./dist/index.js"
  },
  "scripts": {
    "clean": "bun run scripts/clean.ts",
    "build": "bun run clean && bun build src/index.ts --outdir dist --target node",
    "start": "bun run build && ./dist/index.js --quiet --checkSecurity"
  },
  "dependencies": {
    "minimist": "^1.2.5",
    "lodash": "^4.17.15"
  },
  "overrides": {
    "minimist": "1.2.6",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "5.9.3"
  }
}
